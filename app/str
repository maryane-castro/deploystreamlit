file_id1 = '1xyHv-3Nt-hfn_sMBxuc4k3GPl1ivA4nH'
url1 = f'https://drive.google.com/uc?id={file_id1}'
destination1 = 'models/pdi/'
os.makedirs(os.path.dirname(destination1), exist_ok=True)
file_downloaded1 = os.path.exists(os.path.join(destination1, 'recortes.pt'))
if not file_downloaded1:
    gdown.download(url1, destination1, quiet=False)
    file_downloaded1 = True


